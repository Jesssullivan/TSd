{
  "name": "@tummycrypt/tsd",
  "version": "0.1.4",
  "type": "module",
  "description": "Next-Generation Translation System for SvelteKit",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./vite": {
      "types": "./vite/index.d.ts",
      "import": "./vite/index.js"
    },
    "./svelte": {
      "types": "./svelte/index.d.ts",
      "import": "./svelte/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jesssullivan/TSd.git"
  },
  "keywords": [
    "translation",
    "i18n",
    "svelte",
    "sveltekit",
    "localization"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  }
}